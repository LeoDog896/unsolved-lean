{
	"name": "unsolved-lean",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "PUBLIC_BUILD_TIME=$(date) PUBLIC_COMMIT=$(git rev-parse HEAD) vite dev",
		"build:internal": "PUBLIC_BUILD_TIME=$(date) PUBLIC_COMMIT=$(git rev-parse HEAD) vite build",
		"build": "cp ./src/scraper/repositories.json ./static/ && pnpm run build:internal",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"scrape": "tsx src/scraper/index.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@fontsource/atkinson-hyperlegible": "^5.1.1",
		"@shikijs/langs": "^2.1.0",
		"@shikijs/themes": "^2.1.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@types/node": "^22.12.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"highlightjs-lean": "^1.2.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"sass": "^1.83.4",
		"shiki": "^2.1.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tsx": "^4.19.2",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.0.0",
		"yocto-spinner": "^0.2.0"
	}
}
